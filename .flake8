[flake8]
max-line-length = 120
extend-ignore = E203,W503,E402,E401,E721,W293,E127,E501
exclude =
    .git,
    __pycache__,
    build,
    dist,
    venv,
    .venv,
    *.egg-info,
    .eggs,
    catboost_info,
    adk/catboost_info,
    web_ui/catboost_info
per-file-ignores =
    __init__.py:F401
